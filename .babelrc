{
  "presets": [
    [
      "env",
      {
        "modules": "commonjs"
      }
    ],
    "stage-2"
  ],
  "plugins": [
    "syntax-dynamic-import",
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ]
  ]
}
