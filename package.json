    "serve:watch": "npm run build:ssr && npm run serve-watch:inner",
    "analyze": "babel-node build --analyzing",
    "dev": "babel-node build --dev",
    "build": "babel-node build",
    "build:dll": "webpack --config ./build/webpack.dll.babel.js",
    "build:client-standalone": "webpack --config ./build/webpack.standalone.babel.js",
    "serve": "npm run build:ssr && rollup -c server/rollup.config.js && node server/es5-index.js",
